pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                echo 'Building the project...'
                bat 'echo Build step executed'
            }
        }
        stage('Test') {
            steps {
                echo 'Running tests...'
                bat 'echo Test step executed'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying application...'
                bat 'echo Deploy step executed'
            }
        }
    }
}
